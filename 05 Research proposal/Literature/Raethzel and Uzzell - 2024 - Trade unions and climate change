            function checkforie() {
                window.isIE = false;
                var ua = window.navigator.userAgent;
                var old_ie = ua.indexOf('MSIE ');
                var ie_11 = ua.indexOf('Trident/');

                if ((old_ie > -1) || (ie_11 > -1)) {
                    window.isIE = true;
                }
            }
            checkforie();
            window.onerror = onwinerror
