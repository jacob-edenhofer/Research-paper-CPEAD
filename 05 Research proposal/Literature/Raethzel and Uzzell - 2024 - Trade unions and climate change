            function captureErrorCode(errorcode) {
                if (errorcode !== undefined && errorcode != null) {
                    window['turnstileClientErrors'].push("(" + errorcode + ")@" + new Date().toISOString())
                    unhideClientErrors()
                }
            }

            function retryTurnstileOrError(reason) {
                if(window['initialisationFailed']) {
                    return;
                }
                
                if (isSubmitted()) {
                    return;
                }
                
                window['turnstileRetry'] = window['turnstileRetry'] + 1
                try {                    
                    if (window['turnstileRetry'] <= window['maxRetries']) {
                        turnstile.reset(widgetid);
                        turnstile.execute(); 
                    } else {
                        unhideProblem(null);
                    }
                } catch(error) {
                    unhideProblem(error);
                }
            }
            
            function isDefined(data) {
                if (data === null || typeof data === "undefined") {
                    return false;
                }
                return true;
            }
            
            function renderTurnstile() {
                widgetid = turnstile.render('#turnstileForm', {
                    'sitekey': '0x4AAAAAAAIJB5yGm8wJK_4V',
                    'action' : 'spdf-no-referer',
                    'theme': 'light',
                    'appearance': 'always',
                    'execution' : 'execute',
                    'retry-interval': 2000,  
                    'retry': 'never', 
                    'refresh-expired': 'manual',
                    'timeout-callback' : function(errorcode) {
                        captureErrorCode(errorcode)
                        retryTurnstileOrError('timeout');
                    },               
                    'expired-callback' : function(errorcode) {
                        captureErrorCode(errorcode)
                        retryTurnstileOrError('expired');
                    },               
                    'error-callback': function(errorcode) {
                        captureErrorCode(errorcode)
                        setTimeout( function () {
                            retryTurnstileOrError('error');
                        },2000) 
                    },
                    'callback': function (token) {
                        formSubmit();
                    }
                });
            }
        
            window.onloadTurnstileCallback = function() {
                try {
                    renderTurnstile();
                    // try again          
                    if (widgetid === null || widgetid === undefined) {
                       renderTurnstile() 
                    } 
                    if (widgetid === null || widgetid === undefined) {
                        unhideProblemCreationIssue();
                    } else {                    
                        window['turnstileRetry']=0
                        turnstile.execute();
                    }
                } catch (error) {
                    unhideProblemCreationIssue(error);
                }
            }
            
            unhideCss();
            unhideImages();
            window['maxRetries'] = 2;
            window['initialisationFailed'] = false;
            window['turnstileComplete'] = false;
            window['turnstileClientErrors'] = [];
            
            if(window.isIE) {
                unhideBrowserSupport();
            } else {
                unhideChallenge();
                try {
                    turnstile.ready(onloadTurnstileCallback);               
                } catch (error) {
                    unhideProblemCreationIssue(error);
                }
            }
        </script>
