        <script>
            function unhideImages() {
              var images = document.getElementsByClassName("lazyimg");
              for (var i = 0; i < images.length; i++) {
                if (
                    images[i].id !== "failedcschallpx" && 
                    images[i].id !== "oldchallpx"  && 
                    images[i].id !== "errorchallpx" &&
                    images[i].id !== "initerrorchallpx" 
                    ) { 
                    var imagelink = images[i].getAttribute("data-src");
                    images[i].setAttribute("src", imagelink);
                }
              }
            }
            
            function unhideSpecificImage(id) {
                var images = document.getElementsByClassName("lazyimg");
                for (var i = 0; i < images.length; i++) {
                    if (images[i].id === id) { 
                        var imagelink = images[i].getAttribute("data-src");
                        images[i].setAttribute("src", imagelink);
                    }
                }
            }
            
            function unhideFailedClientSideImage() {
                unhideSpecificImage("failedcschallpx")                
            }
            
            function unhideFailedInitialisationImage() {
                unhideSpecificImage("initerrorchallpx")                
            }
            
            function unhideOldImage() {
                unhideSpecificImage("oldchallpx")                
            }
            
            function unhideFailedClientSideWithErrorImage() {
                unhideSpecificImage("errorchallpx")                
            }
            
            function unhideBrowserSupport() {
                unhideOldImage();
                var challenge = document.querySelector(".challenge");
                challenge.classList.add("hidden");
                
                var challenge = document.querySelector(".oldbrowserproblem");
                challenge.classList.remove("hidden");
            }
            
            function unhideProblemCreationIssue(err) {
                window['initialisationFailed'] = true;
                unhideFailedInitialisationImage();
                
                // grab problem div
                var problem = document.querySelector(".challenge");
                var title = problem.querySelector(".title")
                title.innerHTML="Request Verification Initialisation Error";
                
                var form = document.querySelector(".captcha-form");
                form.classList.add("hidden");
                
                if (err !== null && err !== undefined) {
                    var errStack = document.querySelector("#errstack");

                    // hide it
                    errStack.classList.remove("hidden");
                    
                    var browserError = document.querySelector(".browsererror");
                    browserError.innerHTML = "" + err;
                }
                
                if (window['scriptloaderror']) {
                    document.querySelector("#browsersupport").classList.remove("hidden");
                }
            }
            
            function unhideClientErrors() {
                var clienterror = document.querySelector("#clienterror")
                    clienterror.innerHTML="<strong>Error(s)</strong>: " + window['turnstileClientErrors'].toString() + "<br/>"
                    clienterror.classList.remove("hidden");
            }
            
            function unhideChallenge() {
                var challenge = document.querySelector(".challenge");
                challenge.style.display = "block"
            }
            
            function unhideProblem(err) {
                if (window['turnstileClientErrors'].length > 0) {
                    console.log(window['turnstileClientErrors'])
                    unhideClientErrors()
                }
                // grab problem div
                var problem = document.querySelector(".challenge");
                var title = problem.querySelector(".title")
                if (err===null && err === undefined) {
                    title.innerHTML="Request Verification Failed";
                } else {
                    title.innerHTML="Request Verification Error";
                }
                
                if (err !== null && err !== undefined) {
                    unhideFailedClientSideWithErrorImage();
                    var errStack = document.querySelector("#errstack");

                    // hide it
                    errStack.classList.remove("hidden");
                    
                    var browserError = document.querySelector(".browsererror");
                    browserError.innerHTML = "" + err;
                } else {
                    unhideFailedClientSideImage();
                }
            }
            
            function unhideCss() {
              var styleCSS = document.createElement("link");
              styleCSS.rel = "stylesheet";
              // eslint-disable-next-line
              styleCSS.href = "https://sdfestaticassets-eu-west-1.sciencedirectassets.com/shared-assets/25/css/react-header-and-footer/12.0.4/styles.css";
