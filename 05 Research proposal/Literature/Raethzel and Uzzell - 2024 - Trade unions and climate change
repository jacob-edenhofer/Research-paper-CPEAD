          function isSubmitted() {
              if (isDefined(window['turnstileComplete']) && window['turnstileComplete'] === true) {
                  return true;
              } else {
                  return false;
              }
          }
          
          function isNotSubmitted() {
              return !isSubmitted();
          }
        
