map(c("adoption", "stringency"), function(x){
if(grepl("adoption", x)){
map(c("LEV1", "LEV2"), function(y){
save_data(get(paste0("oecd_", x, "_list"))[[y]], paste0("oecd_", x, "_", y))
})
} else {
map(c("LEV1", "LEV2", "LEV3", "LEV4"), function(y){
save_data(get(paste0("oecd_", x, "_list"))[[y]], paste0("oecd_", x, "_", y))
})
}
})
